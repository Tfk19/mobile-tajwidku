{
  "expo": {
    "name": "Tajwidku",
    "slug": "Tajwidku",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashta.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tfk19.Tajwidku"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "my-scheme",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "411f5a2d-97c0-49de-b3cf-a0271935776b"
      }
    }
  }
}
